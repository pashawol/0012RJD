mixin sEvents()
	// start sEvents
	+b.sEvents.pt-0.section#sEvents&attributes(attributes)
		.container
				
				//- +e.btn.btn-primary(href="#") 
				//- 	+icon('icon-print')
				mixin item(d,t, p, z)
					+e.item
						+e.star.active
							+icon('star')
						.row
							.col
								+e.head 
									.row 
										+e.time.col-auto.pb-1!= d 
										block
								.h4.fw-500: a.text-white(href="#")!= t
								if p
									.h6!= p
							if z
								.col-xl-3.col-sm-auto
									a(href="#")
										img(src="img/@2x/zoom.png" loading="lazy", alt="")

				+item(`12.55 – 14.10`, `Переговорная №1`,'','z')
					.pb-1.col-auto Встреча/переговоры
				+item(`12.55 – 14.10`, `In Railway We Trust.`,`Глобальное сотрудничество в условиях неопределенности`) 
					.pb-1.col-auto.text-light Пленарная дискуссия
					.pb-1.col-auto ЗАЛ «ИННОВАЦИИ»
				+item(`12.55 – 14.10`, `Finance as a Masterpiece.`,`Как привлечь инвестиции в условиях кризиса?`) 
					.pb-1.col-auto.text-light Пленарная дискуссия
					.pb-1.col-auto ЗАЛ «ИННОВАЦИИ»

	// end sEvents
mixin sEvents8()
	// start sEvents
	+b.sEvents--page.section#sEvents&attributes(attributes)
		.container
				mixin content()
					.tabs__content&attributes(attributes)
						.tabs-inner
							.tabs-inner__caption
								block
							div
								mixin content2()
									.tabs-inner__content&attributes(attributes)
										mixin item(d,t, c)
											+e.item
												.row
													.col.small!= d
													.col-auto.order-sm-last 
														+e.star(class= c)
															+icon('star') 
												.h6.strong: a.text-body(href="#") Studiare in Italia открывает ворота Италии
												.text-secondary Studiare in Italy: your main door to Italian education
										+e.group
											+e.group-time 14:00
											+item(`14:00 - 14:30`, `Распределенная продажа: предпосылки и&nbsp;перспективы`) 
										+e.group
											+e.group-time 14:30
											+item(`14:00 - 14:30`, `Распределенная продажа: предпосылки и&nbsp;перспективы`, 'active') 
										+e.group
											+e.group-time 15:00
											+item(`14:00 - 14:30`, `Распределенная продажа: предпосылки и&nbsp;перспективы`) 
										+e.group
											+e.group-time 14:00
											+item(`14:00 - 14:30`, `Распределенная продажа: предпосылки и&nbsp;перспективы`) 
										+e.group
											+e.group-time 15:30
											+item(`14:00 - 14:30`, `Распределенная продажа: предпосылки и&nbsp;перспективы`) 
										+e.group
											+e.group-time 16:00
											+item(`14:00 - 14:30`, `Распределенная продажа: предпосылки и&nbsp;перспективы`) 
										+e.group
											+e.group-time 16:30
											+item(`14:00 - 14:30`, `Распределенная продажа: предпосылки и&nbsp;перспективы`) 
								+content2()(class="active")
								+content2()
				+content()(class="active")
					.tabs-inner__btn.active 16.10.2020
					.tabs-inner__btn 17.10.2020
				+content()
					.tabs-inner__btn.active 20.10.2020
					.tabs-inner__btn 21.10.2020

				+content()
					.tabs-inner__btn.active 23.10.2020
					.tabs-inner__btn 24.10.2020
		
	// end sEvents