mixin sMeetingRooms()
	// start sMeetingRooms
	+b.sMeetingRooms#sMeetingRooms&attributes(attributes)
		.container
			.row
				mixin item(n, f)
					+e.col.col-6.col-md-auto
						+e.item(href="#")&attributes(attributes)
							+e.head
								+e.DIV.title Переговорная комната
								if f
									.toggle-el.text-primary.strong Свободна
								else
									.text-primary.strong
							+e.DIV.numb= n
							+e.footer
								if block
									block
								else
									+e.footer-text.toggle-el  Переговорная занята до 18:00
				+item('#1', 'f')(class="sMeetingRooms__item--free")
					+e.btn.toggle-el Забронировать
				+item('#2')
				+item('#3')
				+item('#4')
				+item('#5')
				+item('#6')
				+item('#7')
				+item('#8')
				+item('#9')
				+item('#10')

	// end sMeetingRooms